{
	"name": "parse-html-comments",
	"version": "1.3.1",
	"description": "Get html comments and their details from a string",
	"author": {
		"name": "Cam Sith"
	},
	"engines": {
		"node": ">=4"
	},
	"repository": {
		"url": "https://github.com/csmith14/parse-html-comments"
	},
	"files": [
		"index.js",
		"index.d.ts"
	],
	"keywords": [
		"html",
		"comment",
		"regex",
		"parse"
	],
	"types": "index.d.ts",
	"main": "index.js",
	"scripts": {
		"test": "ava",
		"test:reset": "node test/util/write-result-to-file.js && cp test/util/parse-result.json test/expected.json",
		"tidy": "rimraf .nyc_output, test/return-value.json",
		"lint:circle": "circleci config validate .circleci/config.yml"
	},
	"husky": {
		"hooks": {
			"pre-push": "circleci config validate .circleci/config.yml"
		}
	},
	"dependencies": {},
	"devDependencies": {
		"@camsmith145/eslint-config-standard": "^1.1.1",
		"ava": "^2.4.0",
		"eslint": "^6.8.0",
		"husky": "^4.0.7",
		"rimraf": "^3.0.0"
	},
	"ava": {
		"files": [
			"test/**/*",
			"!test/print-output.js",
			"!test/util"
		],
		"sources": [
			"index.js"
		],
		"verbose": true,
		"failFast": true
	},
	"license": "MIT"
}
